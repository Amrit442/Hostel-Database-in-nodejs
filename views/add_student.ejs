<%- include layouts/header.ejs %>

        <div class="page-data">
            
            <form method="post" action="/students/add">
                <table cellpadding="11">
                 <tr>
                    <td class="label">USN</td><td>: <input type="text" name="usn" required></td>
                 </tr>
                <tr>
                    <td class="label">Name</td><td>: <input type="text" name="name" required></td>
                 </tr>
                 <tr>
                 <tr>
                    <td class="label">Age</td><td>: <input type="text" name="age" required></td>
                 </tr>
                 <tr>
                    <td class="label">Gender</td><td>: <input type="text" name="gender" required></td>
                 </tr>
                 <tr>
                    <td class="label" valign="top">Address</td><td valign="top">: <textarea name="address" required cols="30" rows="5"></textarea></td>
                </tr>
                <tr>
                    <td class="label">Email</td><td>: <input type="text" name="email" required></td>
                 </tr>
                  <tr>
                    <td class="label">Phone</td><td>: <input type="text" name="phone" required></td>
                 </tr>
                 <tr>
                    <td class="label">Fees</td><td>: <input type="text" name="fees" required></td>
                 </tr>
                 <tr>
                    <td class="label">Block</td><td>: <input type="text" name="block" required></td>
                 </tr>
                 <tr>
                    <td class="label">Romm No.</td><td>: <input type="text" name="roomno" required></td>
                 </tr>
                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>
            
        </div>
        
<%- include layouts/footer.ejs %>
